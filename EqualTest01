# 2020-02-06-KaoDian

//equal 和 == 的区别
public class EqualTest01 {
    public static void main(String[] args) {
        //定义3个字符串对象
        String a="1234";
        String b="1234";
        String c=new String("1234");
        //3种不同的布尔判定
        System.out.println(a==b);           //true
        System.out.println(a==c);           //false
        System.out.println(a.equals(c));    //true
    }
}
